{
  "name": "prayertimepro",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "husky install",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "prettier-eslint --write \"src/**/*.ts\" \"src/**/*.tsx\"",
    "tsc": "tsc",
    "validate": "yarn lint && yarn tsc",
    "bundle:android": "react-native bundle --dev false --platform android --entry-file index.js --bundle-output ./android/app/src/main/assets/index.android.bundle --assets-dest ./android/app/src/main/res",
    "detox:test:local": "detox test -c android.attached.debug",
    "detox:emulator": "detox test -c android.emulator.debug",
    "detox:build": "detox build"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "prettier-eslint --write"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.4.0",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.1",
    "axios": "^0.27.2",
    "date-fns": "^2.28.0",
    "deprecated-react-native-prop-types": "^2.3.0",
    "expo": "^46.0.16",
    "expo-sqlite": "^10.3.0",
    "react": "18.1.0",
    "react-native": "0.70.4",
    "react-native-gesture-handler": "^2.8.0",
    "react-native-onboarding-swiper": "^1.2.0",
    "react-native-permissions": "^3.6.1",
    "react-native-reanimated": "^2.12.0",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.18.2",
    "react-native-splash-screen": "^3.3.0",
    "react-native-ui-lib": "6.20.4",
    "react-native-vector-icons": "^9.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-typescript": "^7.17.12",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@testing-library/react-native": "^11.3.0",
    "@types/jest": "^27.0.0",
    "@types/jest-when": "^3.5.0",
    "@types/react": "^18.0.12",
    "@types/react-native": "^0.67.8",
    "@types/react-native-onboarding-swiper": "^1.1.4",
    "@types/react-native-vector-icons": "^6.4.10",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^27.0.0",
    "detox": "^19.7.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.0",
    "jest-html-reporters": "^3.0.9",
    "jest-when": "^3.5.1",
    "lint-staged": "^13.0.1",
    "metro-react-native-babel-preset": "^0.67.0",
    "prettier-eslint-cli": "^7.1.0",
    "react-test-renderer": "18.1",
    "typescript": "^4.7.3",
    "websql": "^2.0.3"
  }
